class Material:
	materiais = {}

	def __init__(self, nome, disciplina):
		self.nome = nome
		self.disciplina = disciplina

	def definir_nome(self, nome):
		self.nome = nome
	
	def definir_disciplina(self, disciplina):
		self.disciplina = disciplina
		
	def alocar_bolsista(self, bolsista):
		self.bolsista = bolsista	

